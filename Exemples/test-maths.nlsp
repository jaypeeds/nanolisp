(PI)
(DE rad-deg(radians) (* 180 (/ radians P)))
(DE deg-rad(degres) (* PI (/ degres 180)))
(DE =(x y) (EQ 0 (- x y)))
(PRINT 'Comparaison_PI_et_4_x_ARC_TANGENTE_1)
(= (deg-rad (* 4 (ATAN 1))) PI)
(DE long-arc(degres rayon) (* rayon (deg-rad degres)))
(PRINT 'Longueur_arc_90_deg_rayon_5.0)
(long-arc 90 5)
(DE matrice-rot(degres) (CONS(SIN degres) (CONS(COS degres) (CONS(-(COS degres)) (CONS (SIN degres) ())))))
(PRINT 'Matrice_de_rotation_de_30_degres)
(matrice-rot 30)
(DE hypotenuse(X Y) (RAC (+ (* X X) (* Y Y))))
(PRINT 'hypotenuse_du_triangle_de_cotes_3_et_4)
(hypotenuse 3 4)
